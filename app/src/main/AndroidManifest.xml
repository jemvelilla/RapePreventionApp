<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.build1.rapepreventionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Welcome" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".RegisterStep1" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".RegisterStep2" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".RegisterStep3" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".RegisterStep4" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".AccountCreated" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Login" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Slides" android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>